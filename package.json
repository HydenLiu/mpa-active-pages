{
  "name": "mpa-active-pages",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rsbuild preview",
    "dev:rsdoctor": "cross-env RSDOCTOR=true rsbuild dev",
    "build:rsdoctor": "cross-env RSDOCTOR=true rsbuild build",
    "build:analyze": "cross-env BUNDLE_ANALYZE=true rsbuild build",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@lucky-canvas/react": "^0.1.13",
    "@radix-ui/react-dialog": "^1.1.15",
    "i18next": "^25.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rsbuild/core": "^1.5.0",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rsdoctor/rspack-plugin": "^1.2.3",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fs-extra": "^11.3.1",
    "globals": "^16.3.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rsbuild-plugin-tailwindcss": "^0.2.3",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "preserveUnused": [
      "commit-msg"
    ]
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  }
}
